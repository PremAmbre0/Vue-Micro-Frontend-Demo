{
  "name": "vite-federation-workspace",
  "private": true,
  "type": "module",
  "packageManager": "npm@10.9.2",
  "workspaces": [
    "shell-app",
    "demo-one-app",
    "demo-two-app",
    "demo-three-app",
    "demo-counter-app"
  ],
  "scripts": {
    "dev": "dotenv -e .env.local -- turbo run dev --parallel",
    "build": "dotenv -e .env.local -- turbo run build",
    "build:sandbox": "dotenv -e .env.sandbox -- turbo run build",
    "preview": "dotenv -e .env.local -- turbo run preview --parallel",
    "preview:sandbox": "dotenv -e .env.sandbox -- turbo run preview --parallel",
    "setup:sandbox": "node scripts/setup-sandbox.js",
    "setup:local": "node scripts/setup-local-env.js",
    "clean": "turbo run clean",
    "install-all": "npm install && turbo run install",
    "install:clean": "npm run clean && rm -rf node_modules package-lock.json && npm install"
  },
  "devDependencies": {
    "dotenv-cli": "^7.3.0",
    "turbo": "^2.5.5"
  },
  "dependencies": {
    "@codesandbox/utils": "^1.0.2"
  }
}
