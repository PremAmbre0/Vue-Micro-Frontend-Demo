{
  "version": 1,
  "name": "vue-micro-frontend-demo",
  "builds": [
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/shell-app/(.*)",
      "dest": "/shell-app/$1"
    },
    {
      "src": "/demo-one-app/(.*)",
      "dest": "/demo-one-app/$1"
    },
    {
      "src": "/demo-two-app/(.*)",
      "dest": "/demo-two-app/$1"
    },
    {
      "src": "/demo-three-app/(.*)",
      "dest": "/demo-three-app/$1"
    },
    {
      "src": "/demo-counter-app/(.*)",
      "dest": "/demo-counter-app/$1"
    },
    {
      "src": "/shell-app",
      "dest": "/shell-app/index.html"
    },
    {
      "src": "/demo-one-app",
      "dest": "/demo-one-app/index.html"
    },
    {
      "src": "/demo-two-app",
      "dest": "/demo-two-app/index.html"
    },
    {
      "src": "/demo-three-app",
      "dest": "/demo-three-app/index.html"
    },
    {
      "src": "/demo-counter-app",
      "dest": "/demo-counter-app/index.html"
    },
    {
      "src": "/",
      "dest": "/shell-app/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/shell-app/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/(.*)\\.(js|css|woff|woff2|eot|ttf|otf)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "functions": {
    "dist/**": {
      "includeFiles": "dist/**"
    }
  }
}
