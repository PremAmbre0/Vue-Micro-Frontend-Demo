{
  "version": 2,
  "builds": [
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/shell-app/(.*)",
      "destination": "/shell-app/$1"
    },
    {
      "source": "/demo-one-app/(.*)",
      "destination": "/demo-one-app/$1"
    },
    {
      "source": "/demo-two-app/(.*)",
      "destination": "/demo-two-app/$1"
    },
    {
      "source": "/demo-three-app/(.*)",
      "destination": "/demo-three-app/$1"
    },
    {
      "source": "/demo-counter-app/(.*)",
      "destination": "/demo-counter-app/$1"
    },
    {
      "source": "/shell-app",
      "destination": "/shell-app/index.html"
    },
    {
      "source": "/demo-one-app",
      "destination": "/demo-one-app/index.html"
    },
    {
      "source": "/demo-two-app",
      "destination": "/demo-two-app/index.html"
    },
    {
      "source": "/demo-three-app",
      "destination": "/demo-three-app/index.html"
    },
    {
      "source": "/demo-counter-app",
      "destination": "/demo-counter-app/index.html"
    },
    {
      "source": "/",
      "destination": "/shell-app/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/shell-app/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/(.*)\\.(js|css|woff|woff2|eot|ttf|otf)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
